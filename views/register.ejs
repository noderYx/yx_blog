<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title> Register </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- CSS -->
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=PT+Sans:400,700'>
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Oleo+Script:400,700'>
    <link rel="stylesheet" href="res/assets/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="res/assets/css/style.css">
    <script type="text/javascript" src="res/dataJs/jquery.cookie.js"></script>

    <script type="text/javascript" src="res/dataJs/user.js"></script>
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

</head>

<body>

<div class="header">
    <div class="container">
        <div class="row">
            <div class="logo span4">
                <h1><a href="">BLOG Register <span class="red">.</span></a></h1>
            </div>

            <div class="links span8">
                <a class="home" href="login" rel="tooltip" data-placement="bottom" data-original-title="Home"></a>
            </div>
        </div>
    </div>
</div>

<div class="register-container container">
    <div class="row">
        <div class="register span3">
        </div>
        <div class="register span6">
            <form action="addStaff" method="post" name="form1" enctype='multipart/form-data'>
                <h2>REGISTER TO <span class="red"><strong>BLOG</strong></span></h2>
                <label>nickname</label>
                <input type="text" id="nickname" name="nickname" placeholder="nickname长度在2~12之间">

                <label>password</label>
                <input type="text" id="password" name="password" placeholder="password长度在6~8之间">
                <label>repassword</label>
                <input type="text" id="repassword" placeholder="repassword与password长度一致">

                <label>mobile</label>
                <input type="text" id="mobile" name="mobile" placeholder="enter your mobile...">
                <label>qq</label>
                <input type="text" id="qq" name="qq" placeholder="QQ长度在6~12之间">

                <label>headIcon</label>
                <img src="" id="preview" style="display: block; width: 100px; padding-left:15px;height: 100px;"/>
                <br/> <input type="file" name="headicon" id="doc" onchange="setImagePreview();"/>
                <br/>
                <br/>
                <input type="button" onclick="validate()"value="REGISTER"/>
            </form>
        </div>
    </div>
</div>
<!-- Javascript -->
<script src="res/assets/js/jquery-1.8.2.min.js"></script>
<script src="res/assets/bootstrap/js/bootstrap.min.js"></script>
<script src="res/assets/js/jquery.backstretch.min.js"></script>
<script src="res/assets/js/scripts.js"></script>

</body>

</html>

